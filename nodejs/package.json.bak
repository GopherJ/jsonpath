{
  "name": "jsonpath-rs",
  "version": "0.1.4",
  "description": "It is native binding of JsonPath engine written in Rust",
  "author": "Changseok Han <freestrings@gmail.com>",
  "license": "MIT",
  "keywords": ["jsonpath", "native", "rustlang", "json"],
  "main": "lib/index.js",
  "dependencies": {
    "neon-cli": "^0.2.0",
    "node-pre-gyp": "amilajack/node-pre-gyp#neon-compat",
    "node-pre-gyp-github": "^1.4.3"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "package": "node-pre-gyp package",
    "upload-binary": "node-pre-gyp package && node-pre-gyp-github publish"
  },
  "devDependencies": {
    "mocha": "^6.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freestrings/jsonpath.git"
  },
  "files": [
    "native/index.node",
    "lib"
  ],
  "binary": {
    "module_name": "index",
    "host": "https://github.com/freestrings/jsonpath/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  }
}
